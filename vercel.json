{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://freeway.ap-northeast-2.elasticbeanstalk.com/api/:path*"
    },
    {
      "source": "/api/:path*/",
      "destination": "http://freeway.ap-northeast-2.elasticbeanstalk.com/api/:path*/"
    },
    { "source": "/(.*)", "destination": "/" }
  ]
}
